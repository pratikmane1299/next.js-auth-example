{
  "compilerOptions": {
		"outDir": "./dist",
    "target": "es2016",
    "module": "commonjs",
		"sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,
    "skipLibCheck": true
  },
	"exclude": [
    "node_modules"
  ],
	"include": [
		"src/**/*.ts"
	]
}
